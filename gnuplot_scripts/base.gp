#-------------------------------------------------------------------------------
# gnuplotの設定
#-------------------------------------------------------------------------------
reset
set nokey                 # 凡例の非表示
# set xrange[-1.6e8:1.6e8]         # y軸方向の範囲の設定
# set yrange[-1.6e8:1.6e8]       # x軸方向の範囲の設定
set xrange[-385000-12000:-385000+12000]         # y軸方向の範囲の設定
set yrange[-12000:+12000]       # x軸方向の範囲の設定
set size ratio 1

set term gif animate delay 0.01      # 出力をgifアニメに設定
set output "lunar.gif"  # 出力ファイル名の設定
#-------------------------------------------------------------------------------
# 変数の設定
#-------------------------------------------------------------------------------
n0 = 240    # ループ変数の初期値
n1 = 10000   # ループ変数の最大値
dn = 20	    # ループ変数の増加間隔

#-------------------------------------------------------------------------------
# ループの開始
#-------------------------------------------------------------------------------
load "gnuplot_scripts/base.plt" 
